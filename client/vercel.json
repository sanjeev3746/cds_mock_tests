{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "build": {
    "env": {
      "CI": "false"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://cds-mock-tests.onrender.com/api/:path*"
    }
  ]
}
